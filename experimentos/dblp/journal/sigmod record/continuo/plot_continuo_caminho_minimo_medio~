#set ytics 0,1,6
#set xtics 1
#set xrange [1976:2011]
#set xtics 3
set xtics 1976,3,2011
set xlabel "Periodo" 
plot "jidm_dados_continuos.txt" using 1:3 t"Caminho Minimo Medio" with linespoints lt 1 lw 2 ps 1 pt 4 lc rgb "red"
