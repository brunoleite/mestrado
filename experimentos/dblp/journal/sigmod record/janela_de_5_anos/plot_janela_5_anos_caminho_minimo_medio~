set encoding iso_8859_1
set output 'sigmod_record_dados_janela_5_anos_caminho_minimo_medio.eps'
set title 'Dados com janela de 5 anos - Caminho Minimo Medio - 1976-2011'
set grid
set key left
#set key spacing 2
set ylabel "Average Shortest Path Length"
set yrange [0:14]
#set ytics 5
#set xtics 1
#set xrange [1976:2011]
#set xtics 3
#set xtics 1976,3,2011
#set xtics("Tempo de processamento" 0,"Tempo de execução" 1)
set xlabel "Periodo"
#set xtics rotate by -90 
set xtics border in scale 1,0.5 nomirror rotate by -25 offset character 0, 0, 0
plot "sigmod_record_dados_janela_5_anos.txt" using 3:xticlabels(1) t"Caminho Minimo Medio" with linespoints lt 1 lw 2 ps 1 pt 4 lc rgb "red"
